{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    },
    "db:generate": {
      "outputs": [
        "node_modules/.prisma/**",
        "**/prisma/generated/**"
      ]
    },
    "db:migrate": {
      "outputs": []
    },
    "db:studio": {
      "cache": false,
      "persistent": true
    }
  }
}
